	//编辑
	function Edit(id){
		$("edit:selid").value = id;
		$("edit:updateflag").value = "EDIT";
		showDiv("编辑单位");
		$("edit:edit").click();
	}
	
	//点击修改角色按钮时调用的方法
	function edit_show(){	
		$("edit:csid").disabled="true";
		Gwin.setLoadok("unitWin");
	}

	//显示层
	function showDiv(title){
		Gwin.open({
			id:"unitWin",	
			title:title,
			contextType:"ID",
			context:"edit",
			dom:document,
			width:420,
			height:170,
			autoLoad:false,
			showbt:false,
			lock:true
		}).show("unitWin");
		if(title.indexOf("添加")!=-1)Gwin.setLoadok("unitWin");	
	}
	
	//隐藏层
	function hideDiv(){
		Gwin.close("unitWin");
	}
	
	function clearData(){
   		if($('list:sk_na')!=null){
   			$('list:sk_na').value="";
   		}
