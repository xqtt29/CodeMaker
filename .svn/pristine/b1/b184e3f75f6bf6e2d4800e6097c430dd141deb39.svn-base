	var Obj=''; 
	
	function doSubmit(){
		if(event.keyCode==13)
		{
			document.forms[0].submit();
		}
	}
	
	function startDo(msg){
		Gwin.progress(msg,10,document);
	}
	function end(){
		alert($("list:msg").value);
		Gwin.close("progress_id");
	}
	function endDo(){
		Gwin.close("progress_id");
		var message = $("list:msg").value;
		var type = "Y";
		if(message.indexOf("成功")!=-1){
			clearText();
			Gwin.close("unitWin");
		}else{ 
			type = "X";
		}
		Gwin.alert("系统提示",message,type,document);
	}

	var delstate = false;
	function deleteAll(obj)	{	
		if(delstate){
			delstate = false;
			return true;
		}else{
			var arr = Gtable.getselectid(obj);
			if(arr.length>0){
				Gwin.confirm("showMsg2","系统提示","确定要删除选中的数据吗?","?",document,
					[{lable:'确定',click:function(){
						$("list:sellist").value = arr;
						startDo("正在删除...");
						delstate = true;
						$("list:delButton").onclick();
						Gwin.close("showMsg2");
					}},
					{lable:'取消',click:function(){
						Gwin.close("showMsg2");
					}}]);
			}else{
				Gwin.alert("系统提示","请选择要需要删除的数据!","!",document);
		   }
		}
		return false;	
	}
	//验证id="edit"的form表单				
	function formCheck(){	
		if($("edit:csna").value==null ||$("edit:csna").value.length<=0){
			Gwin.alert("系统提示","费用项名称不能为空!","!",document);
			$("edit:csna").focus();
			return false;
		}	
		startDo("正在保存...");
		return true;
	}

	function addDiv(){
		clearData();
		$("edit:csid").disabled="";//"禁用".
		$("edit:csid").value="";
		$("edit:csna").value="";
		$("edit:updateflag").value = "ADD";
		showDiv("添加单位");
	}

